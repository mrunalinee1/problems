class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int majority;
        int count = 0;
        
        for (int i = 0; i < nums.size(); i++) {
            if (count == 0) {
                majority = nums[i];
                count = 1;
            } else if (nums[i] == majority) {
                count++;
            } else {
                count--;
            }
        }
        
        return majority;
        
    }
};
